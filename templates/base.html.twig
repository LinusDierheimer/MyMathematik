<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>
            {{ title }}
        </title>

        {% if app.environment == 'dev' %}
            <link rel="stylesheet" href="/css/MyMathematik.css"></link>
        {% else %}
            <link rel="stylesheet" href="/css/MyMathematik.min.css"></link>
        {% endif %}

    </head>
    <body>
        
        {% block header %}
            <header class="header" id="header">
                <a href="/" class="title" rel="home">
                    {{ "header.title"|trans }}
                </a>

                <span class="classlinks">
                    Hallo
                </span>

            </header>
        {% endblock %}

        {% block content %}{% endblock %}

        {% block footer %}
            <footer class="footer" id="footer">
                
            </footer>
        {% endblock %}

        {% if app.environment == 'dev' %}
            <script src="/js/jquery-3.3.1.js"></script>
            <script src="/js/vue.js"></script>
            <script src="/js/MyMathematik.js"></script>
        {% else %}
            <script src="/js/jquery-3.3.1.min.js"></script>
            <script src="/js/vue.min.js"></script>
            <script src="/js/MyMathematik.min.js"></script>
        {% endif %}
    </body>
</html>
