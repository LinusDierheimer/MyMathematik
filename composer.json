{
    "type": "project",
    "license": "proprietary",
    "name": "linus-dierheimer/mymathematik",
    "description": "MyMathematik Mathe Website",
    "version": "1.0.0",
    "homepage": "http://mymathematik.com",
    "readme": "./README.md",
    "authors": [
        {
            "name": "Linus Dierheimer",
            "email": "Linus@Dierheimer.de",
            "homepage": "http://github.com/LinusDierheimer/",
            "role": "Main Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "erusev/parsedown": "^1.7",
        "nochso/html-compress-twig": "^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/expression-language": "^4.3",
        "symfony/filesystem": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.3",
        "symfony/security-bundle": "^4.3",
        "symfony/security-csrf": "^4.3",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/validator": "^4.3",
        "symfony/web-link": "^4.3",
        "symfony/web-server-bundle": "^4.3",
        "symfony/webpack-encore-bundle": "^1.2",
        "symfony/yaml": "^4.3",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^4.3"
        }
    }
}
