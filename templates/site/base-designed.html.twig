    <input type="hidden" id="header_mode" value="designed" />
{% endblock %}

{% block fullcontent %}
    <div class="headerimage">
        <img src="{{ asset('/build/images/Header.jpg') }}" alt="HeaderImage" />
    </div>
    {% block designed_content %}
        <div class="content" id="content">
                {% block content %}{% endblock %}
        </div>
    {% endblock %}
{% endblock %}